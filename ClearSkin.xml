<skin xmlns="http://weblookandfeel.com/XmlSkinExtension">

	 <!-- Skin settings -->
    <id>weblaf.web.skin</id>
    <class>com.alee.skin.web.WebSkin</class>
    <supportedSystems>all</supportedSystems>

    <!-- Skin information -->
    <icon>icons/icon.png</icon>
    <title>Light skin</title>
    <description>WebLaF interface skin</description>
    <author>Paul Pacifico</author>

    <!-- Icon sets -->
    <iconSet>com.alee.iconset.WebIconSet</iconSet>

    <!-- Styles placed in strict initialization order -->
    <include>resources/canvas.xml</include>
    <include>resources/image.xml</include>
    <include>resources/label.xml</include>

	<!-- Extension settings -->
    <id>weblaf.demo.skin.extension</id>
    <class>com.alee.demo.skin.AdaptiveExtension</class>
    <extends>weblaf.web.skin</extends>
    <extends>weblaf.dark.skin</extends>
	
	<include nearClass="com.alee.skin.web.WebSkin">resources/skin.xml</include>
	
	 <!-- File list -->
    <style type="list" id="filelist">
        <component class="com.alee.extended.list.WebFileList">
            <layoutOrientation>2</layoutOrientation>
            <generateThumbnails>true</generateThumbnails>
            <preferredColumnCount>5</preferredColumnCount>
            <preferredRowCount>5</preferredRowCount>
            <fileListViewType>icons</fileListViewType>
        </component>
        <ui>
            <selectionStyle>single</selectionStyle>
        </ui>

        <!-- File cell renderer -->
        <style type="panel" id="renderer" extends="transparent">
            <component class="com.alee.extended.list.WebFileListCellRenderer">
                <gap>4</gap>
            </component>

            <!-- File icon component -->
            <style type="label" id="icon" padding="4">
                <component>
                    <horizontalAlignment>0</horizontalAlignment>
                    <verticalAlignment>0</verticalAlignment>
                    <preferredSize>54,54</preferredSize>
                </component>
            </style>

            <!-- File name label -->
            <style type="label" id="name">
                <component class="com.alee.laf.label.WebLabel">
                    <foreground>0,0,0</foreground>
                </component>
            </style>

            <!-- File description label, tile mode only  -->
            <style type="label" id="description">
                <component class="com.alee.laf.label.WebLabel">
                    <foreground>gray</foreground>
                    <preferredWidth>140</preferredWidth>
                </component>
            </style>

            <!-- File size label, tile mode only -->
            <style type="label" id="size">
                <component class="com.alee.laf.label.WebLabel">
                    <foreground>49,77,179</foreground>
                    <horizontalAlignment>10</horizontalAlignment>
                    <preferredWidth>140</preferredWidth>
                </component>
            </style>

        </style>

        <!-- Tile cell renderer -->
        <style type="panel" id="tile-renderer" extends="renderer" padding="5,5,5,8">

            <!-- File name label -->
            <style type="label" id="name">
                <component class="com.alee.laf.label.WebLabel">
                    <horizontalAlignment>10</horizontalAlignment>
                    <verticalAlignment>0</verticalAlignment>
                    <preferredWidth>140</preferredWidth>
                </component>
            </style>

        </style>

        <!-- Icon cell renderer -->
        <style type="panel" id="icon-renderer" extends="renderer" padding="5,5,8,5">

            <!-- File name label -->
            <style type="label" id="name">
                <component class="com.alee.laf.label.WebLabel">
                    <horizontalAlignment>0</horizontalAlignment>
                    <verticalAlignment>0</verticalAlignment>
                    <preferredWidth>80</preferredWidth>
                </component>
            </style>

        </style>

        <!-- Cell editor field -->
        <style type="textfield" id="editor" extends="nofocus" />
    </style>

    <!-- Non-opaque file list -->
    <style type="list" id="filelist-non-opaque" extends="filelist">
        <component>
            <opaque>false</opaque>
        </component>
    </style>

    <!-- Transparent file list -->
    <style type="list" id="filelist-transparent" extends="filelist-non-opaque">
        <painter>
            <decorations overwrite="true" />
        </painter>
    </style>

    <!-- File list with tiles view -->
    <style type="list" id="filelist-tiles" extends="filelist">
        <component class="com.alee.extended.list.WebFileList">
            <preferredColumnCount>3</preferredColumnCount>
            <preferredRowCount>6</preferredRowCount>
            <fileListViewType>tiles</fileListViewType>
        </component>
    </style>

    <!-- Non-opaque file list with tiles view -->
    <style type="list" id="filelist-tiles-non-opaque" extends="filelist-tiles">
        <component>
            <opaque>false</opaque>
        </component>
    </style>

    <!-- Transparent file list with tiles view -->
    <style type="list" id="filelist-tiles-transparent" extends="filelist-tiles-non-opaque">
        <painter>
            <decorations overwrite="true" />
        </painter>
    </style>

    <!-- File list with icons view -->
    <style type="list" id="filelist-icons" extends="filelist">
        <component class="com.alee.extended.list.WebFileList">
            <preferredColumnCount>7</preferredColumnCount>
            <preferredRowCount>5</preferredRowCount>
            <fileListViewType>icons</fileListViewType>
        </component>
    </style>

    <!-- Non-opaque file list with icons view -->
    <style type="list" id="filelist-icons-non-opaque" extends="filelist-icons">
        <component>
            <opaque>false</opaque>
        </component>
    </style>

    <!-- Transparent file list with icons view -->
    <style type="list" id="filelist-icons-transparent" extends="filelist-icons-non-opaque">
        <painter>
            <decorations overwrite="true" />
        </painter>
    </style>
	
	<!-- File chooser -->
    <style type="filechooser">
        <painter />
    </style>

    <!-- WebLaF-decorated file chooser dialog -->
    <style type="rootpane" id="filechooser" extends="dialog-decorated" />

    <!-- Main content panel -->
    <style type="panel" id="filechooser">

        <!-- Toolbar -->
        <style type="toolbar" id="decorated-bar" extends="attached-north">
            <component>
                <floatable>false</floatable>
            </component>
            <painter>
                <decorations>
                    <decoration>
                        <WebShape round="0" sides="0,0,0,0" />
                    </decoration>
                </decorations>
            </painter>

            <!-- Toolbar button -->
            <style type="button" id="tool" extends="icon-hover" />

            <!-- History popup list scrollpane -->
            <style type="scrollpane" id="history" extends="popup" />

            <!-- Path field -->
            <style type="panel" id="path" extends="pathfield" />

        </style>
        <style type="toolbar" id="undecorated-bar" extends="decorated-bar">
            <painter>
                <decorations>
                    <decoration visible="false" />
                </decorations>
            </painter>
        </style>

        <!-- Central panel -->
        <style type="panel" id="center">
            <component>
                <background>229,233,238</background>
            </component>

            <!-- Central split -->
            <style type="splitpane" id="center" extends="undecorated">
                <component>
                    <dividerSize>8</dividerSize>
                </component>

                <!-- Files tree scrollpane -->
                <style type="scrollpane" id="nav" extends="hovering">
                    <painter>
                        <decorations>
                            <decoration>
                                <WebShape round="2" sides="1,0,1,1" />
                                <WebShadow type="outer" width="0" />
                            </decoration>
                        </decorations>
                    </painter>

                    <!-- Files tree -->
                    <style type="tree" id="file" extends="filetree-non-opaque" />

                </style>

                <!-- Files list scrollpane -->
                <style type="scrollpane" id="view" extends="hovering">
                    <painter>
                        <decorations>
                            <decoration>
                                <WebShape round="2" sides="1,1,1,0" />
                                <WebShadow type="outer" width="0" />
                            </decoration>
                        </decorations>
                    </painter>

                    <!-- Files list -->
                    <style type="list" id="file-tiles" extends="filelist-tiles-non-opaque" />
                    <style type="list" id="file-icons" extends="filelist-icons-non-opaque" />

                    <!-- Files table -->
                    <style type="table" id="file-table" extends="filetable-non-opaque" />

                </style>

            </style>

        </style>

        <!-- South panel -->
        <style type="panel" id="south" padding="4,4,4,4">
            <painter>
                <decorations>
                    <decoration>
                        <WebShape round="0" />
                        <GradientBackground from="0,0" to="0,0.5">
                            <color>229,233,238</color>
                            <color>white</color>
                        </GradientBackground>
                    </decoration>
                </decorations>
            </painter>

            <!-- Selected files title label -->
            <style type="label" id="selected" extends="shadow" padding="0,4,0,0">
				<painter>
					<decorations>
						<decoration>
							<LabelLayout>
								<LabelIcon constraints="icon" />
								<LabelText constraints="text" color="0,0,0" />
							</LabelLayout>
						</decoration>
					</decorations>
				</painter>
			</style>

            <!-- Control buttons -->
            <style type="button" id="control" />
            <style type="button" id="accept" extends="control" />
            <style type="button" id="cancel" extends="control" />

        </style>

        <!-- Files removal confirmation list -->
        <style type="panel" id="removal" padding="4,4,4,4">
            <component>
                <background>white</background>
            </component>
        </style>

    </style>
	
	<!-- Panel -->
	<style type="panel">
		<component>
			<opaque>true</opaque>
			<background>50,50,50</background>
		</component>
		<painter />
	</style>
	
	<!-- Option pane -->
    <style type="optionpane" padding="15,15,15,15">
        <component>
            <background>50,50,50</background>
        </component>
        <painter />
				
        <!-- Message area -->
        <style type="panel" id="message-area" extends="transparent">

            <!-- Icon label -->
            <style type="label" id="message-icon" extends="icon" />

            <!-- Separator and message area -->
            <style type="panel" id="real-body" extends="transparent">

                <!-- Separator between icon and message -->
                <style type="panel" id="separator" extends="transparent">
                    <component>
                        <preferredSize>15,1</preferredSize>
                    </component>
                </style>

                <!-- Message area -->
                <style type="panel" id="body" extends="transparent">

                    <!-- Message labels -->
                    <style type="label" id="message" />

                </style>

            </style>

        </style>

        <!-- Buttons area -->
        <style type="panel" id="button-area" extends="transparent">

            <!-- Buttons -->
            <style type="button" id="button" padding="2,10,2,10" />
            <style type="button" id="yes" extends="button" />
            <style type="button" id="no" extends="button" />
            <style type="button" id="ok" extends="button" />
            <style type="button" id="cancel" extends="button" />

        </style>

    </style>
	
    <!-- Label -->
    <style type="label">
        <painter>
            <decorations>
                <decoration>
                    <LabelLayout>
                        <LabelIcon constraints="icon" />
                        <LabelText constraints="text" color="245,245,245" />
                    </LabelLayout>
                </decoration>
                <decoration states="disabled">
                    <LabelLayout>
                        <LabelText color="120,120,120" />
                    </LabelLayout>
                </decoration>
            </decorations>
        </painter>
    </style>
	
    <!-- Text pane -->
    <style type="textpane" padding="2,2,2,2">
        <component>
            <opaque>false</opaque>
            <background>245,245,245</background>
            <foreground>0,0,0</foreground>
            <selectedTextColor>245,245,245</selectedTextColor>
            <selectionColor>black</selectionColor>
            <caretColor>white</caretColor>
        </component>
        <painter>
            <inputPromptHorizontalPosition>0</inputPromptHorizontalPosition>
            <inputPromptVerticalPosition>0</inputPromptVerticalPosition>
            <inputPromptForeground>160,160,160</inputPromptForeground>
            <inputPromptOnlyWhenEditable>true</inputPromptOnlyWhenEditable>
            <hideInputPromptOnFocus>true</hideInputPromptOnFocus>
            <decorations>
                <decoration>
                    <BoundsShape />
                    <ColorBackground color="255,255,255" />
                </decoration>
            </decorations>
        </painter>
    </style>
	
	<!-- Text pane transparent -->
    <style type="textpane" id="transparent" padding="2,2,2,2">
        <component>
            <opaque>false</opaque>
            <background>245,245,245</background>
            <foreground>0,0,0</foreground>
            <selectedTextColor>245,245,245</selectedTextColor>
            <selectionColor>black</selectionColor>
            <caretColor>white</caretColor>
        </component>
        <painter>
            <inputPromptHorizontalPosition>0</inputPromptHorizontalPosition>
            <inputPromptVerticalPosition>0</inputPromptVerticalPosition>
            <inputPromptForeground>160,160,160</inputPromptForeground>
            <inputPromptOnlyWhenEditable>true</inputPromptOnlyWhenEditable>
            <hideInputPromptOnFocus>true</hideInputPromptOnFocus>
            <decorations>
                <decoration>
                    <BoundsShape />
                    <ColorBackground color="50,50,50,100" />
                </decoration>
            </decorations>
        </painter>
    </style>
	
	<!-- Password field -->
    <style type="passwordfield" padding="0">
        <component>
            <opaque>false</opaque>
            <foreground>black</foreground>
            <selectedTextColor>black</selectedTextColor>
            <selectionColor>154,178,206</selectionColor>
            <caretColor>gray</caretColor>
        </component>
        <painter>
            <inputPromptHorizontalPosition>10</inputPromptHorizontalPosition>
            <inputPromptVerticalPosition>1</inputPromptVerticalPosition>
            <inputPromptForeground>160,160,160</inputPromptForeground>
            <inputPromptOnlyWhenEditable>true</inputPromptOnlyWhenEditable>
            <hideInputPromptOnFocus>false</hideInputPromptOnFocus>
            <decorations>
                <decoration>
                    <WebShape round="2" />
                    <WebShadow type="outer" width="0" />
                    <LineBorder color="170,170,170" />
                    <ColorBackground color="white" />
                </decoration>
                <decoration states="focused">
                    <LineBorder color="85,130,190" />
                </decoration>
                <decoration states="disabled">
                    <WebShadow type="outer" opacity="0" />
                    <LineBorder color="lightGray" />
                </decoration>
            </decorations>
        </painter>
    </style>
	
	<!-- Slider -->
    <style type="slider" padding="0,2,0,2">
        <component>
            <opaque>false</opaque>
        </component>
        <painter>
            <trackHeight>9</trackHeight>
            <trackRound>4</trackRound>
            <trackShadeWidth>0</trackShadeWidth>
            <progressRound>4</progressRound>
            <progressShadeWidth>0</progressShadeWidth>
            <thumbWidth>8</thumbWidth>
            <thumbHeight>18</thumbHeight>
            <thumbRound>2</thumbRound>
            <thumbAngleLength>4</thumbAngleLength>
            <trackBgTop>35, 35, 35</trackBgTop>
            <trackBgBottom>45, 45, 45</trackBgBottom>
            <progressTrackBgTop>129, 198, 253</progressTrackBgTop>
            <progressTrackBgBottom>71, 163, 236</progressTrackBgBottom>
            <progressBorderColor>128,128,128</progressBorderColor>
            <thumbBgTop>80, 80, 80</thumbBgTop>
            <thumbBgBottom>80, 80, 80</thumbBgBottom>
            <drawProgress>true</drawProgress>
            <drawThumb>true</drawThumb>
            <angledThumb>true</angledThumb>
            <sharpThumbAngle>true</sharpThumbAngle>
            <rolloverDarkBorderOnly>false</rolloverDarkBorderOnly>
            <animated>true</animated>
        </painter>

        <!-- Default labels -->
        <style type="label" id="tick">
            <component>
                <horizontalAlignment>0</horizontalAlignment>
            </component>
        </style>

    </style>
	
	<!-- Progress bar -->
    <style type="progressbar">
        <component>
            <opaque>false</opaque>
        </component>
        <painter>
            <minimumContentSize>100,10</minimumContentSize>
            <decorations>
                <decoration>
                    <WebShape round="4" />
                    <WebShadow type="outer" width="0" />
                    <LineBorder color="80, 80, 80" />
                      <GradientBackground>
                            <color>35, 35, 35</color>
                            <color>45, 45, 45</color>
                      </GradientBackground>
                </decoration>
            </decorations>

            <!-- Progress line -->
            <progressPainter>
                <decorations>
                    <decoration>
                        <WebShape round="4" />
						<WebShadow type="outer" width="0" />
                    </decoration>
                    <decoration states="progress">
                        <GradientBackground>
                            <color>129, 198, 253</color>
                            <color>71, 163, 236</color>
                       </GradientBackground>
                    </decoration>
                    <decoration states="enabled,progress,intermediate">
                        <MovingHighlightBackground id="overlay" orientation="horizontal" width="120" color="71, 163, 236" passes="1" duration="1s" delay="4s" />
                    </decoration>
                    <decoration states="enabled,indeterminate">
                        <MovingHighlightBackground id="overlay" orientation="horizontal" width="120" color="71, 163, 236" passes="2" duration="1.5s" delay="0s" />
                    </decoration>
                </decorations>
            </progressPainter>
        </painter>
    </style>
	
	<!-- Radio -->
	<style type="radiobutton" padding="0">
        <painter>
            <decorations>
                <decoration>
                    <ButtonLayout>
						<ButtonIcon constraints="icon" />
						<StyledButtonText constraints="text" color="245,245,245"/>
                    </ButtonLayout>
                </decoration>
                <decoration states="disabled">
                    <ButtonLayout>
						<StyledButtonText constraints="text" color="35,35,35"/>
                    </ButtonLayout>
                </decoration>
            </decorations>

            <!-- Radio state painter -->
            <checkStatePainter>
                <decorations>
                    <decoration size="16,16">
						<WebShadow type="outer" width="0" />
                        <LineBorder color="80, 80, 80" />
						<GradientBackground type="radial" from="0.5,0.5" to="2,0.5">
                            <color>35, 35, 35</color>
                            <color>45, 45, 45</color>
                        </GradientBackground>
                    </decoration>
                    <decoration states="checked">
						<WebShadow type="outer" width="0" />
						<RadioIcon padding="2" leftColor="71, 163, 236" rightColor="71, 163, 236" />
                        <ColorBackground color="40, 40, 40" />
                    </decoration>
					<decoration states="focused">
						<WebShadow type="outer" width="0" />
                        <LineBorder color="80, 80, 80" />
                    </decoration>
                    <decoration states="hover,enabled">
						<WebShadow type="outer" width="0" />
                        <LineBorder color="80, 80, 80" />
						<ColorBackground color="21, 113, 186" />
                    </decoration>
                    <decoration states="disabled">
						<WebShadow type="outer" width="0" />
                        <LineBorder color="40, 40, 40" />
						<GradientBackground type="radial" from="0.5,0.5" to="2,0.5">
                            <color>35, 35, 35</color>
                            <color>45, 45, 45</color>
                        </GradientBackground>
                    </decoration>
                    <decoration states="disabled,checked">
						<WebShadow type="outer" width="0" />
                        <ColorBackground color="40, 40, 40" />
                    </decoration>
                </decorations>
            </checkStatePainter>
        </painter>
    </style>
			
    <!-- Text area -->
    <style type="textarea" padding="0,0,0,0">
        <component>
            <opaque>true</opaque>
            <background>white</background>
            <foreground>black</foreground>
            <selectedTextColor>black</selectedTextColor>
            <selectionColor>154,178,206</selectionColor>
            <caretColor>gray</caretColor>
        </component>
        <painter>
            <inputPromptHorizontalPosition>0</inputPromptHorizontalPosition>
            <inputPromptVerticalPosition>0</inputPromptVerticalPosition>
            <inputPromptForeground>160,160,160</inputPromptForeground>
            <inputPromptOnlyWhenEditable>true</inputPromptOnlyWhenEditable>
            <hideInputPromptOnFocus>true</hideInputPromptOnFocus>
            <decorations>
                <decoration>
                    <BoundsShape />
                    <ColorBackground color="white" />
                </decoration>
            </decorations>
        </painter>
    </style>
	
	<!-- Tabbed pane -->
    <style type="tabbedpane">
        <component>
            <background>80,80,80</background>
            <layout class="TabbedPaneLayout$UIResource">
                <ContentSize type="preferred" />
            </layout>
        </component>
        <painter>
            <decorations overwrite="true">
                <decoration states="focused" />
            </decorations>
        </painter>

        <!-- Tab area -->
        <style type="panel" id="tab-area" extends="transparent">
            <component>
                <layout class="TabAreaLayout$UIResource" />
            </component>
            <painter>
				<decorations overwrite="true">
                    <decoration>
                        <BoundsShape />
                        <ColorBackground color="50,50,50" />
                        <Stripes orientation="horizontal" align="bottom" bounds="margin">
                            <Stripe>
                                <color>50,50,50</color>
                            </Stripe>
                        </Stripes>
                    </decoration>
                </decorations>
            </painter>

            <!-- Tab viewport -->
            <style type="viewport" id="viewport" extends="transparent">

                <!-- Tab container -->
                <style type="panel" id="container" extends="transparent">

                    <!-- Tab title -->
                    <style type="styledlabel" id="tab" padding="4,5,4,6">
                        <component>
                            <horizontalAlignment>0</horizontalAlignment>
                        </component>
                        <painter>
                            <decorations overwrite="true">
                                <decoration>
                                    <BoundsShape />
                                    <ColorBackground color="40,40,40" />
                                </decoration>
                                <decoration states="left">
                                    <Stripes orientation="vertical" align="right" bounds="margin" />
                                </decoration>
                                <decoration states="right">
                                    <Stripes orientation="vertical" align="left" bounds="margin" />
                                </decoration>
                                <decoration states="bottom">
                                    <Stripes align="top" />
                                </decoration>
                                <decoration states="enabled,hover">
                                    <ColorBackground color="55,55,55" />
                                </decoration>
                                <decoration states="selected">
                                    <ColorBackground color="50,50,50" />
                                </decoration>
                                <decoration states="selected,in-focused-parent">
                                </decoration>
                                <decoration states="has-no-children">
                                    <LabelLayout>
                                        <StyledLabelText constraints="text" color="255,255,255" />
                                    </LabelLayout>
                                </decoration>
                                <decoration states="has-no-children,hover">
                                    <LabelLayout>
                                        <StyledLabelText color="255,255,255" />
                                    </LabelLayout>
                                </decoration>
                                <decoration states="has-no-children,selected">
                                    <LabelLayout>
                                        <StyledLabelText color="255,255,255" />
                                    </LabelLayout>
                                </decoration>
                                <decoration states="has-no-children,selected,in-focused-parent">
                                    <LabelLayout>
                                        <StyledLabelText color="255,255,255" />
                                    </LabelLayout>
                                </decoration>
                                <decoration states="has-no-children,disabled">
                                    <LabelLayout>
                                        <StyledLabelText color="255,255,255" ignoreStyleColors="true" />
                                    </LabelLayout>
                                </decoration>
                            </decorations>
                        </painter>
                    </style>

                </style>

            </style>

            <!-- Tab menu button -->
            <style type="button" id="tab-menu-button" extends="undecorated" padding="0,0,0,0">
                <painter>
                    <decorations>
                        <decoration>
                            <SetIcon constraints="icon" icon="menu-dimmed" />
                        </decoration>
                        <decoration states="hover">
                            <SetIcon constraints="icon" icon="menu-hover" />
                        </decoration>
                    </decorations>
                </painter>

                <!-- Popup style -->
                <style type="popupmenu" id="menu">
                    <painter>
                        <popupStyle>simple</popupStyle>
                        <background>40,40,40</background>
                        <borderColor>80,80,80</borderColor>
                        <round>4</round>
                        <shadeWidth>6</shadeWidth>
                        <shadeOpacity>1</shadeOpacity>
                        <opacity>1</opacity>
                    </painter>

                    <!-- Menu item -->
                    <style type="radiobuttonmenuitem" id="item">
                        <painter>
                            <decorations>
                                <decoration states="selected">
                                    <ColorBackground color="55,55,55" />
									<StyledButtonText padding="0,16,2,0" constraints="text" color="255,255,255"/>
									<Stripes orientation="vertical" align="left" bounds="margin">
                                        <Stripe stroke="basic;5">
                                            <color>55,55,55</color>
                                        </Stripe>
                                    </Stripes>
                                </decoration>
                                <decoration states="checked">
                                    <ColorBackground color="50,50,50" />
									<Stripes orientation="vertical" align="left" bounds="margin">
                                        <Stripe stroke="basic;5">
                                            <color>50, 50, 50</color>
                                        </Stripe>
                                    </Stripes>
								</decoration>
                                <decoration states="checked,selected">
                                    <ColorBackground color="55,55,55" />
									<Stripes orientation="vertical" align="left" bounds="margin">
                                        <Stripe stroke="basic;5">
                                            <color>55,55,55</color>
                                        </Stripe>
                                    </Stripes>
                                </decoration>
								<decoration>
									<StyledButtonText padding="0,16,2,0" constraints="text" color="255,255,255"/>
								</decoration>
						  </decorations>
                        </painter>
                    </style>
                </style>
            </style>
        </style>
    </style>

    <!-- Transparent list -->
    <style type="list">
        <painter>
            <decorations overwrite="true" />
        </painter>
    </style>
	
	 <!-- Text field -->
    <style type="textfield" padding="2">
        <component>
            <opaque>false</opaque>
            <foreground>black</foreground>
            <selectedTextColor>black</selectedTextColor>
            <selectionColor>154,178,206</selectionColor>
            <caretColor>gray</caretColor>
        </component>
        <painter>
            <inputPromptHorizontalPosition>10</inputPromptHorizontalPosition>
            <inputPromptVerticalPosition>1</inputPromptVerticalPosition>
            <inputPromptForeground>160,160,160</inputPromptForeground>
            <inputPromptOnlyWhenEditable>true</inputPromptOnlyWhenEditable>
            <hideInputPromptOnFocus>false</hideInputPromptOnFocus>
            <decorations>
                <decoration>
                    <WebShape round="2" />
                    <WebShadow type="outer" width="0" />
                    <LineBorder color="170,170,170" />
                    <ColorBackground color="white" />
                </decoration>
                <decoration states="focused">
					<WebShadow type="outer" width="0" />
                    <LineBorder color="85,130,190" />
                </decoration>
                <decoration states="disabled">
                    <WebShadow type="outer" width="0" />
                    <LineBorder color="lightGray" />
                </decoration>
            </decorations>
        </painter>
    </style>
	
	<!-- Combobox -->
    <style type="combobox">
        <component>
            <opaque>false</opaque>
        </component>
        <painter>
            <decorations>
                <decoration>
                    <WebShape round="2" />
                    <WebShadow type="outer" width="0" />
                    <LineBorder color="170,170,170" />
                    <GradientBackground>
                        <color>white</color>
						<color>223,223,223</color>
                    </GradientBackground>
                </decoration>
                <decoration states="focused">
					<WebShadow type="outer" width="0" />
                    <LineBorder color="170,170,170" />
                </decoration>
                <decoration states="expanded">
					<WebShadow type="outer" width="0" />
                    <WebShadow type="inner" width="6" />
                    <ColorBackground color="210,210,210" />
                </decoration>
                <decoration states="disabled">
					<WebShadow type="outer" width="0" />
                    <LineBorder color="lightGray" />
                </decoration>
            </decorations>
        </painter>

        <!-- Default editor -->
        <style type="textfield" id="editor">
            <painter>
                <decorations overwrite="true">
                    <decoration>
                        <WebShape round="2,0,0,2" sides="0,0,0,1" />
                        <LineBorder color="170,170,170" />
                        <GradientBackground>
                        <color>white</color>
						<color>223,223,223</color>
                    </GradientBackground>
                    </decoration>
                    <decoration states="focused">
                        <LineBorder color="85,130,190" />
                    </decoration>
                    <decoration states="disabled">
                        <WebShadow type="outer" opacity="0" />
                        <LineBorder color="lightGray" />
                    </decoration>
                </decorations>
            </painter>
        </style>

        <!-- Separator -->
        <style type="separator" id="separator" extends="vertical">
            <painter>
                <decorations overwrite="true">
                    <decoration states="editable">
                        <SeparatorStripes padding="0" overwrite="true" />
                    </decoration>
                </decorations>
            </painter>
        </style>

        <!-- Arrow button -->
        <style type="button" id="arrow" padding="2">
            <painter>
                <decorations>
                    <decoration visible="false">
                        <SetIcon icon="down-small" />
                    </decoration>
                    <decoration states="expanded">
                        <SetIcon icon="up-small" />
                    </decoration>
                </decorations>
            </painter>
        </style>
    </style>
	
	<!-- Spinner -->
    <style type="spinner" padding="0">
        <component>
            <opaque>false</opaque>
			<focusable>false</focusable>
        </component>
        <painter>
            <decorations>
                <decoration>
                    <WebShape round="4" />
                    <WebShadow type="outer" width="0" />
                    <ColorBackground color="white" />
                </decoration>
            </decorations>
        </painter>	
    </style>
	
	<!-- Table -->
    <style type="table">
        <component>
            <opaque>false</opaque>
            <fillsViewportHeight>false</fillsViewportHeight>
            <showHorizontalLines>true</showHorizontalLines>
            <showVerticalLines>true</showVerticalLines>
            <gridColor>120,120,120</gridColor>
        </component>
        <painter>
            <decorations>
                <decoration>
                    <BoundsShape />
                    <ColorBackground color="106,110,112,0" />
                </decoration>
            </decorations>
            
           <!-- Cell selection painter -->
            <selectionPainter>
                <decorations>
                    <decoration>
                        <WebShape round="0" />
                        <GradientBackground>
                            <color>129,198,253</color>
                            <color>129,198,253</color>
                        </GradientBackground>
                    </decoration>
                </decorations>
            </selectionPainter>

            <!-- Row background painter -->
            <rowPainter>
                <decorations>
                    <decoration states="even">
                        <WebShape round="0" />
                        <ColorBackground color="190,190,190" />
                    </decoration>
					<decoration states="odd">
                        <WebShape round="0" />
                        <ColorBackground color="185,185,185" />
                    </decoration>
                </decorations>
            </rowPainter>
        </painter>
    </style>
	
</skin>